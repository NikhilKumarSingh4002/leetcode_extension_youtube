{
  "manifest_version": 3,
  "name": "LeetCode YouTube Solutions (User Key)",
  "version": "1.0.1",
  "description": "Shows YouTube solution videos in LeetCode. Each user provides their own YouTube API key in options.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://leetcode.com/*",
    "https://www.googleapis.com/*"
  ],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "styles.css"
      ],
      "matches": [
        "https://leetcode.com/*"
      ]
    }
  ],
  "icons": {
    "48": "icon48.png"
  }
}